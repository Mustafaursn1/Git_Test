package day28overriding;

public class Cat extends Animal {

	public static void main(String[] args) {
		
		Cat cat=new Cat();
		cat.sound();
		cat.numOfFoot();
		
		
	}
	@Override
	public void numOfFoot() {
		System.out.println("4 Ayaklari var");
	}
	public void sound() {
		System.out.println("Miyavlarlar");
	}

	
}
