package day30;

public class TryCatc02 {
	static String m;

	public static void main(String[] args) {
		
		
		String s ="";
		System.out.println(s.length());//0
		try {
			s+="t";
			
		}catch(Exception e) {
			s+="c";
		}finally {
			s+="f";
		}
		s+="a";
		System.out.println(s);
		//String'e null atandigi zaman length() methodu calismz
		//ve NullPointterException Alirsiniz
		String n=null;
		
		System.out.println(n.length());
	}

}
